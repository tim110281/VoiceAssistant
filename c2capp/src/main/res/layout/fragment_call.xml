<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.ecnetwork.iptnet.net.ui.call.CallFragment">

    <TextView
        android:id="@+id/textView_loginState"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:background="@android:color/holo_red_dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_title_calleeid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:text="Callee Id:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextText_calleeId"
        android:layout_width="326dp"
        android:layout_height="53dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="5dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/text_title_calleeid"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_dial"
        android:layout_width="102dp"
        android:layout_height="41dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="150dp"
        android:text="Dial"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText_calleeId" />
</androidx.constraintlayout.widget.ConstraintLayout>